@model UxCarrier.Models.Dto.EInvoiceBindStep4Dto

<html>

<body onload='document.forms["form"].submit()'>
    <form name='form' action="@ViewData["url"]" method='post'>
        <input type='hidden' name='card_ban' value='@(Model.card_ban)' />
        <input type='hidden' name='card_no1' value='@(Model.card_no1)' />
        <input type='hidden' name='card_no2' value='@(Model.card_no2)' />
        <input type='hidden' name='card_type' value='@(Model.card_type)' />
        <input type='hidden' name='signature' value='@(Model.signature)' />
        <input type='hidden' name='token' value='@(Model.token)' />  
    </form> 
</body>

</html>